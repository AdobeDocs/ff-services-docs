    "paths": {
      "/v2/assets": {
        "post": {
          "summary": "Create an asset",
          "tags": [
            "Assets"
          ],
          "description": "Create an asset. You will receive an `uploadUrl` in the response, which you can use to upload the asset.",
          "operationId": "createAsset",
          "parameters": [

          ],
          "security": [
            {
              "bearer_auth": [

              ],
              "api_key": [

              ]
            }
          ],
          "responses": {
            "401": {
              "description": "Unauthorized",
